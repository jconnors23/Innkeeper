    "dotenv": "^16.0.0",
    "mocha": "^10.0.0",
    "node": "^17.4.0",
    "nodemon": "^2.0.20",
    "ts": "^0.2.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  },
