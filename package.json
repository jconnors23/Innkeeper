  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  },
  "description": "Discord bot that fetches Magic the Gathering Cards from Scryfall and displays them for users.",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "build": "tsc",
